    "setup:s3": "node scripts/setup-s3-buckets.js",
    "test:aws": "node scripts/test-aws-connection.js"
  },
  "keywords": [
    "construction",
    "management",
    "ai",
    "mongodb",
    "express"
  ],
  "author": "Q-Sci Team",
  "license": "MIT",
